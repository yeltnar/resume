  "private": true,
  "dependencies": {
    "puppeteer": "^2.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vitest",
